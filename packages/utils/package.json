{
  "name": "@versa-chat/utils",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./request": {
      "import": "./dist/request/index.js",
      "types": "./dist/request/index.d.ts"
    },
    "./*": {
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist && rm -rf tsconfig.tsbuildinfo",
    "lint": "eslint src --ext .ts,.tsx",
    "build": "tsc --build",
    "dev": "tsc --build --watch",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "axios": "^1.6.7"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "@types/axios": "^0.14.4"
  }
}
